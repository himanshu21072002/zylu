const mongoose=require("mongoose");

const User=mongoose.Schema({
username:{
    type:String,
    required:true,
    unique:true,
},

email:{
    type:String,
    required:true,
},
gender:{
    type:String,
    required:true,
},
location:{
    type:String,
    required:true,
},
age:{
    type:String,
    required:true,
},
experience:{
    type:int,
    required:true,
},
picture:{
    type:String,
    required:true,
},
});

module.exports=mongoose.model("User",User);